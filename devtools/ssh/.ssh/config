Host *
    StrictHostKeyChecking no
    ServerAliveCountMax 3
    ServerAliveInterval 60
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%r@%h-%p
    ControlPersist 10m
    IdentitiesOnly yes
    # HostKeyAlgorithms +ssh-rsa
    # PubkeyAcceptedAlgorithms +ssh-rsa

Include hosts/os/*.conf
Include hosts/local/*.conf
